module Drawer(input logic clk,
				  input logic[9:0] x, y,
				  output logic[3:0] r, g, b);
				  
	always_ff @(posedge clk)
		if (x > 640 | y > 480)
			begin
				r <= 4'b0;
				g <= 4'b0;
				b <= 4'b0;
			end
		else
			begin
				r <= 4'b15;
				g <= 4'b0;
				b <= 4'b0;
			end
			
endmodule