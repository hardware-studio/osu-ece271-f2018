module box_controller(input vs,
							 input logic[2:0] dir,
							 input logic[9:0] x, y,
							 output logic[3:0] box_r, box_g, box_b,
							 output logic[9:0] box_x, box_y, box_size);
							 
	logic[9:0] xpos = 0;
	logic[9:0] ypos = 0;
	
	always_ff @(posedge vs)
		begin
			case(dir)
				0: xpos = xpos - 1;
		end
	
endmodule