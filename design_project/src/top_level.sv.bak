module top_level(input clk,
					  output logic vs, hs,
					  output logic[3:0] r, g, b);
					  
	logic[9:0] xpos, ypos;
	
	XYCounter c (
		.clk(clk),
		.vs(vs),
		.hs(hs),
		.x(xpos),
		.y(ypos)
	);
	
	Drawer d (
		.clk(clk),
		.x(xpos),
		.y(ypos),
		.r(r),
		.g(g),
		.b(b)
	);
	
endmodule